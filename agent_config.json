{
  "agent": {
    "project_dir": "./project",
    "plan_file": "development_plan.md",
    "max_steps": 10,
    "step_delay": 1.0
  },
  "llm": {
    "model_name": "gpt-4",
    "temperature": 0.7,
    "max_tokens": 1000,
    "api_key_env_var": "OPENAI_API_KEY"
  },
  "executor": {
    "command_timeout": 30,
    "max_file_size": 1048576,
    "safe_mode": true,
    "backup_files": true
  }
}
